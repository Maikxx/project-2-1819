  "scripts": {
    "build": "tsc && yarn minify-client && copyfiles -E -u 2 \"./server/src/views/**/*\" ./server/dist",
    "open-chrome": "wait-on http://localhost:3000 && open -a \"Google Chrome\" http://localhost:3000",
    "minify-client": "yarn minify-css && yarn minify-js && yarn precompress",
    "minify-css": "gulp minifyCss",
    "minify-js": "gulp minifyJs",
    "precompress": "gulp precompress",
    "start": "node server/dist/index.js",
    "start-server": "concurrently \"yarn minify-client\" \"yarn watch-server\" \"yarn open-chrome\" --raw",
    "watch-server": "ts-node-dev --respawn --transpileOnly ./server/src/index.ts"
  },
  "devDependencies": {
